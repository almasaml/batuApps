        xmlns:tools="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
                name="listplace"
                type="com.almas.batuapps.menu.listplace.viewmodels.FragmentListplaceViewModel"/>
    </data>

    <RelativeLayout
            android:id="@+id/layoutList"
